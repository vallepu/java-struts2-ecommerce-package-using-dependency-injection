<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  
  <class name="org.webhop.ywdc.beans.ProductArchive" table="productarchive">
  
  <id name="id">
  	<generator class="native"/>
  </id>
  
  <property name="productid"></property>
  <property name="productname"></property>
  <property name="categoryname"></property>
  <property name="price"></property>
  <property name="summary"></property>
  
  <property name="datedeleted" type="timestamp"></property>

  
  
	
  </class>
  
  
</hibernate-mapping>