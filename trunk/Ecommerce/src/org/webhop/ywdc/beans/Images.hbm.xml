<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  
  <class name="org.webhop.ywdc.beans.Images" table="images">
  
  <id name="id">
  	<generator class="native"/>
  </id>
  
 
  <property name="front"></property>
  <property name="back"></property>
  <property name="sidea"></property>
  <property name="sideb"></property>
  <property name="top"></property>
  <property name="bottom"></property>
  
	
  </class>
  
  
</hibernate-mapping>